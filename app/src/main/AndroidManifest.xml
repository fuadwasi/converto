<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.converto">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/converto_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/converto_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".About"></activity>
        <activity android:name=".Weight" />
        <activity android:name=".Distance" />
        <activity android:name=".Area" />
        <activity android:name=".Currency" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>